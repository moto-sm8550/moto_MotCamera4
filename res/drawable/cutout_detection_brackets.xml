<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center|left|top">
        <rotate android:drawable="@drawable/cutout_bracket_corner" android:fromDegrees="0.0" android:toDegrees="0.0" android:pivotX="50.0%" android:pivotY="50.0%" />
    </item>
    <item android:gravity="center|right|top">
        <rotate android:drawable="@drawable/cutout_bracket_corner" android:fromDegrees="90.0" android:toDegrees="90.0" android:pivotX="50.0%" android:pivotY="50.0%" />
    </item>
    <item android:gravity="bottom|center|right">
        <rotate android:drawable="@drawable/cutout_bracket_corner" android:fromDegrees="180.0" android:toDegrees="180.0" android:pivotX="50.0%" android:pivotY="50.0%" />
    </item>
    <item android:gravity="bottom|center|left">
        <rotate android:drawable="@drawable/cutout_bracket_corner" android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%" />
    </item>
</layer-list>