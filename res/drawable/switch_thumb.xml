<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:dither="true" android:visible="true" android:shape="rectangle" android:useLevel="false">
            <solid android:color="?colorPrimary" />
            <corners android:radius="12.5dip" />
            <size android:height="25.0dip" android:width="25.0dip" />
            <stroke android:width="6.0dip" android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="true" android:visible="true" android:shape="rectangle" android:useLevel="false">
            <solid android:color="@android:color/white" />
            <corners android:radius="12.5dip" />
            <size android:height="25.0dip" android:width="25.0dip" />
            <stroke android:width="6.0dip" android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>